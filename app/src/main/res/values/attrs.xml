<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekBar">
        <attr name="trackColor" format="color" />
        <attr name="trackProgressColor" format="color" />
        <attr name="thumbColor" format="color" />
        <attr name="thumbBorderColor" format="color" />

        <attr name="trackHeight" format="dimension" />
        <attr name="thumbRadius" format="dimension" />

        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="leftThumbValue" format="integer" />
        <attr name="rightThumbValue" format="integer" />
    </declare-styleable>
</resources>